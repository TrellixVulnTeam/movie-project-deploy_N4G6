{
  "name": "movie-project",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "docker:build": "docker image build . -t thinkful-ed/zid-be-project-movie-01-qualified",
    "docker:run": "docker run --rm -it -p 5000:5000 thinkful-ed/zid-be-project-movie-01-qualified",
    "docker:stop": "docker stop $(docker ps -q)",
    "docker:test": "docker run -t  thinkful-ed/zid-be-project-movie-01-qualified npm test",
    "knex": "knex",
    "migrate": "knex migrate:latest",
    "reset": "npm run rollback && npm run migrate && npm run seed",
    "rollback": "knex migrate:rollback",
    "seed": "knex seed:run",
    "build": "npm -prefix client run build",
    "preinstall": "cd ./backend && npm install",
    "install": "cd ./client && npm install",
    "start": "npx concurrently 'npm run start:backend' 'npm run start:frontend'",
    "start:backend": "npm --prefix ./backend start",
    "start:frontend": "npm --prefix ./frontend start",
    "start:dev": "npx concurrently \"npm run start:dev --prefix ./backend\" \"npm run start --prefix ./frontend\" ",
    "test": "npm run test:frontend && npm run test:backend",
    "test:backend": "npm test --prefix ./backend",
    "test:frontend": "npm test --prefix ./frontend"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/markdown-it": "^12.2.3",
    "concurrently": "^7.3.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "knex": "^2.2.0",
    "markdown": "^0.5.0",
    "pg": "^8.7.3",
    "react-scripts": "^5.0.1"
  }
}
